{
    "name": "portenko/app-yii2",
    "description": "Admin control panel based on Yii2",
    "keywords": [
        "admin control panel",
        "admin template",
        "admin part",
        "backend part",
        "fast developing",
        "yii2"
    ],
    "authors": [
      {
        "name": "Bogdan Portenko",
        "email": "portenko@gmail.com"
      }
    ],
    "homepage": "https://github.com/portenko/app-yii2",
    "type": "project",
    "license": "MIT",
    "support": {
      "issues": "https://github.com/portenko/app-yii2/issues"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6.0",
        "yiisoft/yii2": "~2.0.14",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "mihaildev/yii2-elfinder": "*",
        "yiisoft/yii2-imagine": "^2.2"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.1.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/codeception": "^4.0",
        "codeception/module-asserts": "^1.0",
        "codeception/module-yii2": "^1.0",
        "codeception/module-filesystem": "^1.0",
        "codeception/verify": "~0.5.0 || ~1.1.0",
        "symfony/browser-kit": ">=2.7 <=4.2.4"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
